  "scripts": {
    "prepare": "yarn run md-docs && cd ../api && yarn run prepare",
    "md-docs": "npx swagger-markdown -i ../api/src/schema/schema.yaml -o ./reference-api.md",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "postbuild": "next-sitemap",
    "type-check": "tsc --pretty --noEmit",
