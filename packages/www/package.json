    "docker:build": "yarn run static-export && docker build -t livepeerci/www:${DOCKER_TAG:-master} .",
    "docker:push": "docker push livepeerci/www:${DOCKER_TAG:-master}",
    "static-export": "export LP_STATIC_BUILD=true; next build; next export || true"
  },
